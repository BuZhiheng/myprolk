<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:background="@color/color_green"
        >
        <TextView
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/main_mine"
            android:textSize="@dimen/text_main_title"
            android:textColor="@color/color_white"
            />
    </FrameLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_minefrm_user_msg"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:gravity="center"
                android:background="@drawable/ic_minefrm_photo_bg"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/iv_minefrm_photo"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/ic_launcher"/>
                <TextView
                    android:id="@+id/tv_minefrm_nickname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:text="未登录"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_white"/>
                <TextView
                    android:id="@+id/tv_minefrm_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="点击登录"
                    android:textSize="@dimen/text_common_item_content"
                    android:textColor="@color/color_white"/>
            </LinearLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_jifen"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="我的积分"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <TextView
                    android:id="@+id/tv_minefrm_jifen"
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="39dp"
                    android:text=""
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"
                    android:visibility="gone"
                    />
            </FrameLayout>
            <include layout="@layout/include_slid_img_marginleft"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_msg"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_chatroom_newmsg"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="我的消息"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <!--<include layout="@layout/include_slid_img_marginleft"/>-->
            <!--<FrameLayout-->
                <!--android:id="@+id/fl_minefrm_wz"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="50dp"-->
                <!--android:background="@drawable/common_ll_bg"-->
                <!--android:clickable="true"-->
                <!-->-->
                <!--<ImageView-->
                    <!--android:layout_gravity="center_vertical"-->
                    <!--android:layout_width="25dp"-->
                    <!--android:layout_height="25dp"-->
                    <!--android:layout_marginLeft="@dimen/common_margin"-->
                    <!--android:src="@drawable/ic_minefrm_wz"-->
                    <!--/>-->
                <!--<TextView-->
                    <!--android:layout_gravity="center_vertical"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_marginLeft="50dp"-->
                    <!--android:text="违章查询"-->
                    <!--android:textSize="@dimen/text_common_item_title"-->
                    <!--android:textColor="@color/color_black_text"-->
                    <!--/>-->
