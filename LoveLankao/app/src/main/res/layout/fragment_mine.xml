                    <!--android:layout_gravity="center_vertical|right"-->
                    <!--android:layout_width="20dp"-->
                    <!--android:layout_height="20dp"-->
                    <!--android:layout_marginRight="@dimen/common_margin"-->
                    <!--android:src="@drawable/ic_common_right_green"/>-->
            <!--</FrameLayout>-->
            <include layout="@layout/include_slid_img_height10"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_needpartner"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_needpartner"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="我要合作"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <include layout="@layout/include_slid_img_marginleft"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_tuijian"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_tuijian"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="今日推荐"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <include layout="@layout/include_slid_img_marginleft"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_mypartner"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_mypartner"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="合作商家"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <include layout="@layout/include_slid_img_marginleft"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_aboutus"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_aboutas"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="关于我们"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <include layout="@layout/include_slid_img_height10"/>
            <FrameLayout
                android:id="@+id/fl_minefrm_setting"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/common_ll_bg"
                android:clickable="true"
                >
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="@dimen/common_margin"
                    android:src="@drawable/ic_minefrm_setting"
                    />
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:text="设置"
                    android:textSize="@dimen/text_common_item_title"
                    android:textColor="@color/color_black_text"
                    />
                <ImageView
                    android:layout_gravity="center_vertical|right"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="@dimen/common_margin"
                    android:src="@drawable/ic_common_right_green"/>
            </FrameLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/color_bg"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>